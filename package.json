    "lint": "eslint 'src/**/*' 'lib/**/*' 'gatsby/**/*'",
    "prettier": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "check-types": "tsc",
    "test": "yarn check-types && jest",
    "clean": "gatsby clean",
    "firebase": "firebase",
    "generate-scripts": "cd lib && yarn build",
    "check-config": "yarn generate-scripts && node lib/build/index.js check-config",
